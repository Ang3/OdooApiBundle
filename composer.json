{
    "name": "ang3/odoo-api-bundle",
    "type": "symfony-bundle",
    "description": "This bundles is an integration of the Odoo external API client: ang3/php-odoo-api-client",
    "keywords": ["odoo", "api", "client", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Joanis ROUANET",
            "email": "joanis.ang3@gmail.com",
            "homepage": "https://github.com/Ang3"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ang3\\Bundle\\OdooApiBundle\\": "src/"
        }
    },
    "require": {
        "php": "^7.1",
        "ang3/php-odoo-api-client": "^5.0",
        "symfony/dependency-injection": ">=3.2.0",
        "symfony/config": ">=3.2.0",
        "symfony/http-kernel": ">=3.2.0",
        "symfony/validator": ">=3.2.0",
        "symfony/expression-language": "^5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "ang3/php-dev-binaries": "^1.0"
    }
}
