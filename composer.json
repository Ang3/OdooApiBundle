{
    "name": "ang3/odoo-api-bundle",
    "type": "symfony-bundle",
    "description": "This bundles is an integration of the Odoo external API client: ang3/php-odoo-api-client",
    "keywords": ["odoo", "api", "external", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Joanis ROUANET",
            "email": "joanis.ang3@gmail.com",
            "homepage": "http://www.lixye.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ang3\\Bundle\\OdooApiBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1.3",
        "ang3/php-odoo-api-client": "^3.0",
        "ang3/php-odoo-orm": "dev-master",
        "symfony/dependency-injection": "^4.1",
        "symfony/http-kernel": "^4.1",
        "doctrine/orm": "^2.6",
        "doctrine/reflection": "^1.0"
    },
    "require-dev": {
        "hautelook/alice-bundle": "^2.5"
    }
}
